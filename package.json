  "scripts": {
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "create-migration": "npm run typeorm migration:create --config src/config/typeorm.config.ts -n",
    "run-migrations": "npm run typeorm migration:run --config src/config/typeorm.config.ts",
    "revert-migrations": "npm run typeorm migration:revert --config src/config/typeorm.config.ts ",
    "generate-migrations": "npm run typeorm migration:generate --config src/config/typeorm.config.ts ",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nodemon --config nodemon.json",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
